extends layout

block main-content
  - let pName = park.querySelector("LocationName").textContent;
  - let pAddress = park.querySelector("Address").textContent;
  - let pFacil = park.querySelectorAll("FacilityDisplayName");
  h1 #{pName}
  p #{pAddress}
  p Facilities: 
  ul 
    each f in pFacil 
      li #{f.textContent}